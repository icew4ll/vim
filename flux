# install f.lux
sudo add-apt-repository ppa:nathan-renniewaldock/flux
sudo apt-get update
sudo apt-get install fluxgui

# f.lux gui
fluxgui

# lat long
47.218125, -122.483622
xflux -k 4200 -l 47.21869367, -g -122.48350382
sudo xflux -k 2700 -l -77 -g 166

# redshift
sudo apt-get install redshift

# permanent manual set color temp 1700k
redshift -O 1700


